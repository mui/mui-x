    "@mui/internal-code-infra": "^0.0.3-canary.37",
    "@mui/internal-markdown": "^2.0.11",
    "@mui/internal-netlify-cache": "^0.0.2-canary.1",
    "@mui/internal-test-utils": "catalog:",
    "@mui/material": "catalog:",
    "@mui/monorepo": "github:mui/material-ui#57ae83d02aecf2eeb8cff71cdf8c28ae7b6ea0e5",
    "@mui/utils": "catalog:",
    "@next/eslint-plugin-next": "15.5.6",
    "@octokit/plugin-retry": "^8.0.2",
    "@octokit/rest": "^22.0.0",
    "@playwright/test": "catalog:",
    "@types/babel__core": "^7.20.5",
    "@types/babel__traverse": "^7.28.0",
    "@types/chai-dom": "^1.11.3",
    "@types/node": "^22.18.12",
    "@types/react": "catalog:",
    "@types/react-dom": "catalog:",
    "@types/requestidlecallback": "^0.3.7",
    "@types/sinon": "^17.0.4",
    "@types/yargs": "^17.0.34",
    "@typescript-eslint/parser": "catalog:",
    "@vitejs/plugin-react": "catalog:",
    "@vitest/browser": "catalog:",
    "@vitest/coverage-v8": "catalog:",
    "@vvago/vale": "^3.12.0",
    "axe-core": "4.11.0",
    "babel-loader": "^10.0.0",
    "babel-plugin-react-remove-properties": "catalog:",
    "babel-plugin-search-and-replace": "^1.1.1",
    "babel-plugin-transform-react-remove-prop-types": "catalog:",
    "babel-plugin-transform-replace-expressions": "^0.2.0",
    "chai-dom": "^1.12.1",
    "concurrently": "^9.2.1",
    "cross-env": "catalog:",
    "danger": "^13.0.4",
    "date-fns-jalali-v2": "npm:date-fns-jalali@2.30.0-0",
    "date-fns-v2": "npm:date-fns@2.30.0",
    "es-toolkit": "^1.41.0",
    "esbuild": "^0.25.11",
    "eslint": "^9.38.0",
    "eslint-import-resolver-webpack": "^0.13.10",
    "eslint-plugin-consistent-default-export-name": "^0.0.15",
    "eslint-plugin-jsdoc": "^61.1.7",
    "eslint-plugin-mui-x": "workspace:^",
    "execa": "^9.6.0",
    "glob-gitignore": "^1.0.15",
    "globby": "^15.0.0",
    "jsdom": "26.1.0",
    "lerna": "^9.0.0",
    "magic-string": "^0.30.21",
    "markdownlint-cli2": "^0.18.1",
    "moment": "catalog:",
    "moment-timezone": "catalog:",
    "null-loader": "^4.0.1",
    "nyc": "^17.1.0",
    "prettier": "^3.6.2",
    "pretty-quick": "^4.2.2",
    "process": "^0.11.10",
    "react": "catalog:",
    "react-dom": "catalog:",
    "remark": "^15.0.1",
    "rimraf": "catalog:",
    "serve": "catalog:",
    "sinon": "^21.0.0",
    "stream-browserify": "^3.0.0",
    "string-replace-loader": "^3.2.0",
    "stylelint": "^16.25.0",
    "stylelint-config-tailwindcss": "^1.0.0",
    "terser-webpack-plugin": "^5.3.14",
    "tsx": "catalog:",
    "typescript": "^5.9.3",
    "unist-util-visit": "^5.0.0",
    "util": "^0.12.5",
    "vite": "catalog:",
    "vitest": "catalog:",
    "vitest-fail-on-console": "^0.10.1",
    "yargs": "catalog:"
  },
  "resolutions": {
    "ast-types": "^0.14.2"
  },
  "packageManager": "pnpm@10.19.0",
  "engines": {
    "pnpm": "10.19.0",
    "node": ">=22.18"
  },
  "pnpm": {
    "packageExtensions": {
      "@mui/internal-test-utils": {
        "dependencies": {
          "lodash.kebabcase": "^4.1.1"
        }
      }
    }
  }
}
