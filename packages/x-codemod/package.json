{
  "name": "@mui/x-codemod",
  "version": "8.5.2",
  "author": "MUI Team",
  "description": "Codemod scripts for MUI X.",
  "bin": "./codemod.js",
  "keywords": [
    "react",
    "react-component",
    "mui",
    "mui-x",
    "codemod",
    "jscodeshift"
  ],
  "scripts": {
    "typescript": "tsc -p tsconfig.json",
    "prebuild": "rimraf build",
    "copy-files": "cpy README.md build && cpy package.json build",
    "build": "node ../../scripts/build.mjs node --ignore 'src/types.ts' && pnpm copy-files"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mui/mui-x.git",
    "directory": "packages/x-codemod"
  },
  "license": "MIT",
  "homepage": "https://github.com/mui/mui-x/tree/master/packages/x-codemod",
  "funding": {
    "type": "opencollective",
    "url": "https://opencollective.com/mui-org"
  },
  "dependencies": {
    "@babel/core": "catalog:",
    "@babel/runtime": "catalog:",
    "@babel/traverse": "catalog:",
    "@mui/x-internals": "workspace:*",
    "jscodeshift": "catalog:",
    "yargs": "catalog:"
  },
  "devDependencies": {
    "@types/jscodeshift": "^0.12.0",
    "dayjs": "catalog:",
    "moment-timezone": "catalog:",
    "rimraf": "catalog:"
  },
  "sideEffects": false,
  "publishConfig": {
    "access": "public",
    "directory": "build"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
