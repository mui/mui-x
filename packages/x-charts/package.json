    "bench:jsdom": "pnpm vitest bench -c ./vitest.config.jsdom.mts",
    "bench:browser": "pnpm vitest bench -c ./vitest.config.browser.mts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mui/mui-x.git",
