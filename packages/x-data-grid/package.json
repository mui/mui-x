{
  "name": "@mui/x-data-grid",
  "version": "8.9.1",
  "author": "MUI Team",
  "description": "The Community plan edition of the MUI X Data Grid components.",
  "main": "src/index.ts",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mui/mui-x/issues"
  },
  "homepage": "https://mui.com/x/react-data-grid/",
  "sideEffects": [
    "**/*.css"
  ],
  "funding": {
    "type": "opencollective",
    "url": "https://opencollective.com/mui-org"
  },
  "publishConfig": {
    "access": "public",
    "directory": "build"
  },
  "keywords": [
    "react",
    "react-component",
    "material-ui",
    "mui",
    "mui-x",
    "react-table",
    "table",
    "datatable",
    "data-table",
    "datagrid",
    "data-grid"
  ],
  "scripts": {
    "typescript": "pnpm typescript:src && pnpm typescript:test",
    "typescript:src": "tsc -p tsconfig.json",
    "typescript:test": "tsc -p tsconfig.test.json",
    "build": "pnpm build:node && pnpm build:stable && pnpm build:types && pnpm build:copy-files",
    "build:node": "node ../../scripts/build.mjs node --babel-flag=\"--copy-files\" --babel-flag=\"--no-copy-ignored\"",
    "build:stable": "node ../../scripts/build.mjs stable --babel-flag=\"--copy-files\" --babel-flag=\"--no-copy-ignored\"",
    "build:copy-files": "node ../../scripts/copyFiles.mjs",
    "build:types": "tsx ../../scripts/buildTypes.mts --removeCss",
    "prebuild": "rimraf build tsconfig.build.tsbuildinfo"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mui/mui-x.git",
    "directory": "packages/x-data-grid"
  },
  "dependencies": {
    "@babel/runtime": "catalog:",
    "@mui/utils": "catalog:",
    "@mui/x-internals": "workspace:*",
    "clsx": "catalog:",
    "prop-types": "catalog:",
    "use-sync-external-store": "catalog:"
  },
  "peerDependencies": {
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@mui/material": "^5.15.14 || ^6.0.0 || ^7.0.0",
    "@mui/system": "^5.15.14 || ^6.0.0 || ^7.0.0",
    "react": "^17.0.0 || ^18.0.0 || ^19.0.0",
    "react-dom": "^17.0.0 || ^18.0.0 || ^19.0.0"
  },
  "peerDependenciesMeta": {
    "@emotion/react": {
      "optional": true
    },
    "@emotion/styled": {
      "optional": true
    }
  },
  "devDependencies": {
    "@mui/internal-test-utils": "catalog:",
    "@mui/material": "catalog:",
    "@mui/system": "catalog:",
    "@mui/types": "^7.4.4",
    "@types/prop-types": "catalog:",
    "@types/use-sync-external-store": "catalog:",
    "react": "catalog:",
    "react-dom": "catalog:",
    "rimraf": "catalog:"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
