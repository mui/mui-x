{
  "name": "@mui-x-internal/test-regressions",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "cross-env NODE_ENV=production pnpm build && concurrently --success first --kill-others \"pnpm run runn\" \"pnpm run serverr\"",
    "build": "cross-env TEST_BUILD=true vite build --config vite.config.js",
    "dev": "pnpm test:regressions:build && pnpm test:regressions:server",
    "runn": "mocha --config .mocharc.cjs '**/*.test.{js,ts,tsx}'",
    "serverr": "serve . -p 5001"
  },
  "devDependencies": {
    "@babel/runtime": "^7.27.0",
    "@playwright/test": "^1.52.0",
    "@types/chai": "^4.3.20",
    "@types/karma": "^6.3.9",
    "@types/moment-jalaali": "^0.7.9",
    "@types/react": "^19.0.10",
    "@types/react-router": "^5.1.20",
    "@types/semver": "^7.7.0",
    "@vitejs/plugin-react": "^4.4.1",
    "chai": "^4.5.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router": "^7.5.2",
    "react-transition-group": "^4.4.5",
    "tsx": "^4.19.3"
  }
}
