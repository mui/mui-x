{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "isolatedModules": true,
    /* files are emitted by babel */
    "noEmit": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "types": ["react", "node"],
    "incremental": true,
    "noImplicitAny": true
  },
  "include": ["src/pages/**/*", "data/**/*"],
  "exclude": ["docs/.next", "docs/export"]
}
