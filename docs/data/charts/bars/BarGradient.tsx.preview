<BarChart
  {...settings}
  sx={{
    [`& .${barClasses.series}[data-series="2"] .${barElementClasses.root}`]: {
      fill: 'url(#bar-gradient)',
    },
    [`& .${barClasses.seriesLabels}[data-series="2"] .${barLabelClasses.root}`]:
      {
        fontWeight: 'bold',
      },
  }}
>
  <defs>
    <Gradient id="bar-gradient" />
  </defs>
</BarChart>