<ToggleButtonGroup
  value={chartType}
  exclusive
  onChange={handleChartType}
  aria-label="chart type"
  fullWidth
>
  {['bar', 'area'].map((type) => (
    <ToggleButton key={type} value={type}  >
      {type}
    </ToggleButton>
  ))}
</ToggleButtonGroup>
{chartType === 'bar' && <SymlogBarChart />}
{chartType === 'area' && <SymlogAreaChart />}