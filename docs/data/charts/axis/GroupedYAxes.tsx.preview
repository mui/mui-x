<BarChart
  layout="horizontal"
  yAxis={[
    {
      data: categoryData,
      scaleType: 'band',
      width: 130,
      groups: [
        // Extract main category
        { getValue: (category) => category.split(' - ')[1] },
        // Extract subcategory
        {
          getValue: (category) => category.split(' - ')[0],
          tickSize: 130,
        },
      ],
      valueFormatter: (value: string) => value.split(' - ')[1] || value,
    },
  ]}
  {...chartConfig}
/>