<FormControlLabel
  checked={isNumerical}
  control={
    <Checkbox onChange={(event) => setIsNumerical(event.target.checked)} />
  }
  label="is numerical"
  labelPlacement="end"
/>
<BarChartPro
  {...barSettings}
  xAxis={[{ ...xAxis, scaleType: 'band', zoom, tickNumber: 4, isNumerical }]}
/>
<LineChartPro
  {...lineSettings}
  xAxis={[{ ...xAxis, scaleType: 'point', zoom, tickNumber: 4, isNumerical }]}
/>